    <speed>2755.9</speed>
    <repeat>true</repeat>
    <delay>0.15</delay>
    <yaw-offset>0</yaw-offset>
    <pitch-offset>0</pitch-offset>
    <life>40</life>
    <wind>false</wind>
    <weight>0.286600941</weight>
    <cd>0.193</cd>
    <eda>0.00338158219</eda>
    <fuse-range>0</fuse-range>
    <collision>true</collision>
