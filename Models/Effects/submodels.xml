    <z-offset>  0.4302 </z-offset>
    <yaw-offset>315</yaw-offset>
    <pitch-offset>0</pitch-offset>
    <life>5</life>
    <buoyancy>8</buoyancy>
    <eda>0.2</eda>
    <collision>false</collision>
  </submodel>

