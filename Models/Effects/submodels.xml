    <model>Aircraft/aw139/Models/Effects/Flares/flare-complet.xml</model>
    <trigger>/controls/armament/flares-release</trigger>
    <speed>40</speed>
    <repeat>true</repeat>
    <delay>0.4</delay>
    <count>-1</count>
    <x-offset> -20.1444 </x-offset>
