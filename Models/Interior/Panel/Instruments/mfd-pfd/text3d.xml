<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  
  <text>
    <name>AP-status</name>
    <offsets>
      <x-m>  0.000 </x-m>
      <y-m> -0.035 </y-m>
      <z-m>  0.086 </z-m>
    </offsets>
    <alignment>center-center</alignment>
    <axis-alignment>yz-plane</axis-alignment>
    <type type="string">text-value</type>
    <property>autopilot/locks/AP-status</property>
    <font>helvetica_bold.txf</font>
    <character-size>0.007</character-size>
    <character-aspect-ratio type="double">1.4</character-aspect-ratio>
    <font-resolution>
      <width type="int">64</width>
      <height type="int">64</height>
    </font-resolution>
  </text>
  
  <text>
    <name>AP-vert-armed</name>
    <offsets>
      <x-m> 0.000 </x-m>
      <y-m> 0.025 </y-m>
      <z-m> 0.101 </z-m>
    </offsets>
    <alignment>center-center</alignment>
    <axis-alignment>yz-plane</axis-alignment>
    <type type="string">text-value</type>
    <property>autopilot/locks/altitude-arm</property>
    <font>helvetica_bold.txf</font>
    <character-size>0.007</character-size>
    <character-aspect-ratio type="double">1.4</character-aspect-ratio>
    <font-resolution>
      <width type="int">64</width>
      <height type="int">64</height>
    </font-resolution>
  </text>
  
  <text>
    <name>Nav-id</name>
    <offsets>
      <x-m>  0.000 </x-m>
      <y-m>  0.025 </y-m>
      <z-m> -0.014 </z-m>
    </offsets>
    <alignment>left-center</alignment>
    <axis-alignment>yz-plane</axis-alignment>
    <type>text-value</type>
    <property>autopilot/internal/nav-id</property>
    <format>%0.5s</format>
    <font>helvetica_bold.txf</font>
    <character-size>0.006</character-size>
    <character-aspect-ratio type="double">1.3</character-aspect-ratio>
    <font-resolution>
      <width type="int">64</width>
      <height type="int">64</height>
    </font-resolution>
  </text>
  
  <animation>
    <name>white</name>
    <type>material</type>
    <object-name>AP-status</object-name>
    <object-name>AP-vert-armed</object-name>
    <object-name>Nav-id</object-name>
    <emission>
      <red-prop>sim/color-set/white/r</red-prop>
      <green-prop>sim/color-set/white/g</green-prop>
      <blue-prop>sim/color-set/white/b</blue-prop>
      <alpha-prop>sim/color-set/white/a</alpha-prop>
    </emission>
    <ambient>
      <red>   0 </red>
      <green> 0 </green>
      <blue>  0 </blue>
    </ambient>
    <diffuse>
      <red>   0 </red>
      <green> 0 </green>
      <blue>  0 </blue>
    </diffuse>
    <specular>
      <red>   0 </red>
      <green> 0 </green>
      <blue>  0 </blue>
    </specular>
  </animation>
  
</PropertyList>
