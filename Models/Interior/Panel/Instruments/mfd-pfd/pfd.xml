  <animation>
    <name>Attitude indicator</name>
    <type>texmultiple</type>
    <object-name>att</object-name>
    <transform>
      <subtype>textranslate</subtype>
      <property>orientation/pitch-deg</property>
      <interpolation>
        <entry><ind>-90.0</ind><dep>-0.41</dep></entry>
        <entry><ind>0.0</ind><dep>0.0</dep></entry>
        <entry><ind>90.0</ind><dep>0.41</dep></entry>
      </interpolation>
      <axis>
        <x> 0 </x>
        <y> 1 </y>
      </axis>
    </transform>
    <transform>
      <subtype>texrotate</subtype>
      <property>orientation/roll-deg</property>
      <center>
        <x> 0.25 </x>
        <y> 0.50 </y>
      </center>
      <axis>
        <x>  0 </x>
        <y>  0 </y>
        <z> -1 </z>
      </axis>
    </transform>
  </animation>

  <!-- hsi rose -->
    <animation>
    <type>select</type>
    <object-name>ptr1</object-name>
    <condition>
      <greater-than>
        <property>instrumentation/primus2000/sc840/nav1ptr</property>
        <value>0</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>ptr1label-vor</object-name>
    <condition>
      <equals>
        <property>instrumentation/primus2000/sc840/nav1ptr</property>
