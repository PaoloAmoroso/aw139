<PropertyList>

  <path>chip_det_panel.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>cablecutswitch</object-name>
    <object-name>chipdetswitch</object-name>
    <object-name>clutchguard</object-name>
    <object-name>forcetrimswitch</object-name>
    <object-name>hydcontswitch</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>fond</object-name>
    <object-name>cablecutswitch</object-name>
    <object-name>chipdetswitch</object-name>
    <object-name>clutchguard</object-name>
    <object-name>forcetrimswitch</object-name>
    <object-name>hydcontswitch</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>fond</object-name>
    <object-name>cablecutswitch</object-name>
    <object-name>chipdetswitch</object-name>
    <object-name>clutchguard</object-name>
    <object-name>forcetrimswitch</object-name>
    <object-name>hydcontswitch</object-name>
    <condition>
      <property>controls/electric/battery-switch</property>
    </condition>
    <emission>
      <factor-prop>controls/lighting/instruments-norm</factor-prop>
      <red>   0.50 </red>
      <green> 0.40 </green>
      <blue>  0.30 </blue>
    </emission>
  </animation>

