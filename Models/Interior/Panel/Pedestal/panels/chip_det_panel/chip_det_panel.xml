  <!-- Clutch -->
  <animation>
    <type>knob</type>
    <object-name>clutchguard</object-name>
    <property>controls/switches/cablecutclutchguard</property>
    <factor>-40</factor>
    <center>
      <x-m> -0.01970 </x-m>
      <y-m> -0.01149 </y-m>
      <z-m>  0.00006 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
    <action>
      <binding>
        <command>property-toggle</command>
        <property>controls/switches/cablecutclutchguard</property>
      </binding>
    </action>
  </animation>

