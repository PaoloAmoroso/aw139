      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
    <factor>-0.014</factor>
    <action>
      <binding>
        <command>property-toggle</command>
        <property>controls/circuit_breaker/attd_pilot_1</property>
      </binding>
    </action>
  </animation>

